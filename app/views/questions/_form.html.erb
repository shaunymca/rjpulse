<%= simple_form_for(@question) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :survey_id %>
    <%= f.input :question_s %>
    <%= f.input :question_t %>
    <%= f.input :answer_type %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
