<%= simple_form_for(@answer) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :user_id %>
    <%= f.input :question_id %>
    <%= f.input :value_s %>
    <%= f.input :value_t %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
